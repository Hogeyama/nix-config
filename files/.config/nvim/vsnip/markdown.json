{
  "code": {
    "prefix": [
      "code"
    ],
    "body": [
      "```${1:lang}",
      "${2}",
      "```"
    ]
  },
  "h1": {
    "prefix": [
      "h1"
    ],
    "body": [
      "================================================================================"
    ]
  },
  "h2": {
    "prefix": [
      "h2"
    ],
    "body": [
      "---------------------------------------"
    ]
  },
  "hl": {
    "prefix": [
      "hl"
    ],
    "body": [
      "-------------------------------------------------------------------------------"
    ]
  },
  "pre": {
    "prefix": [
      "pre",
      "collapse"
    ],
    "body": [
      "<div class=\"collapse\">",
      "<!-- {{{ -->",
      "```${1}",
      "${2}",
      "```",
      "<!-- }}} -->",
      "</div>"
    ]
  }
}
